{
  "presets": [
    ["env", {
      "targets": {
        "ie": 9,
        "uglify": true,
      },
      "modules": false,
      "loose": true
    }],
    "stage-1",
	  "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-decorators-legacy"
  ],
  "env": {
    "development": {
      "presets": [
        ["env", {
          "modules": "commonjs",
		  "loose": true
        }],
        "stage-1",
    	  "react"
      ],
      "plugins": [
        "transform-class-properties",
		"transform-object-rest-spread",
		"transform-decorators-legacy"
      ]
    }
  }
}
