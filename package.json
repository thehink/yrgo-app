    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "better-npm-run": "^0.0.15",
    "browser-sync": "^2.18.8",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "csswring": "^6.0.0",
